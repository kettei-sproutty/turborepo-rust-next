{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "install:rust": {
      "cache": true,
      "outputs": ["bin/**"]
    },
    "install": {
      "dependsOn": ["install:rust", "^install"],
      "outputs": ["node_modules/**", "pnpm-lock.yaml"]
    },
    "build": {
      "dependsOn": ["install", "^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
